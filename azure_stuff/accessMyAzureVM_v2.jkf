node () {
    environment {
                 AZURE_COMMON_CREDS = credentials('AzurePEMKey')
                 // AZURE_COMMON_CREDS_USR AZURE_COMMON_CREDS_PSW
            }
    stage ('Stage1') {
        sh '''#!/bin/bash -l   
                ssh-keygen -R 52.188.213.251
                ssh-keyscan  52.188.213.251 >> ~/.ssh/known_hosts             
                ssh -i $AZURE_COMMON_CREDS_PSW $AZURE_COMMON_CREDS_USR@52.188.213.251 hostname
            '''    
    }
      

    }

}